<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/vortex/option.xsd">
	<import>
		<categories>
			<category name="extended_search.faq">
				<parent>extended_search</parent>
			</category>
		</categories>
		
		<options>
			<option name="extended_search_faq_enabled">
				<categoryname>extended_search.faq</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>extended_search_faq_enable_path,extended_search_faq_count</enableoptions>
			</option>
			
			<option name="extended_search_faq_enable_path">
				<categoryname>extended_search.faq</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			
			<option name="extended_search_faq_count">
				<categoryname>extended_search.faq</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>10</defaultvalue>
				<minvalue>1</minvalue>
			</option>
		</options>
	</import>
</data>
